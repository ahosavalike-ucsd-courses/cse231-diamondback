(fun (isodd n)
  (if (< n 0)
      (isodd (- 0 n))
      (if (= n 0)
          false
          (iseven (sub1 n))
      )
  )
)
(fun (iseven n)
  (if (= n 0)
      true
      (isodd (sub1 n))
  )
)
(block
  (print input)
  (print (iseven input))
)
; ((input 10) (output 10 true true))
; ((input 9) (output 9 false false))
